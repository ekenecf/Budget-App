<h1>Create new group</h1>
<%= form_with model: @group, class: "form-group", url: user_groups_path, method: :post, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :icon %>
  <%= f.file_field :icon, direct_upload: true %>

  <%= f.submit "Save group" %>
<% end %>
